{
  "name": "sts-blog",
  "version": "0.0.1",
  "description": "A private blog in which I document the results of my research on stock markets and various investment strategies.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "test": "jest",
    "test-coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "version": "echo $npm_package_version"
  },
  "keywords": [
    "Blog",
    "STS",
    "Research",
    "Stock markets",
    "Investment",
    "Investment strategies",
    "Economics"
  ],
  "author": "Alexander Padberg <undefinedhuman>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "commitizen": "^4.0.4",
    "coveralls": "^3.0.11",
    "cz-conventional-changelog": "^3.1.0",
    "jest": "^25.3.0"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "npm": "^6.14.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!data/keyMap.js",
      "!/node_modules/"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
