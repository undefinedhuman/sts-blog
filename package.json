{
  "name": "sts-blog",
  "version": "0.17.0",
  "description": "A private blog where I document the results of my research on stock markets and various investment strategies.",
  "main": "app.js",
  "scripts": {
    "scss": "node-sass --watch public/stylesheets -o public/stylesheets/css",
    "commit": "git-cz",
    "start": "node app.js",
    "dev": "nodemon -q app.js",
    "test": "jest --config=jest.json --watch --coverage",
    "test-coverage": "jest --config=jest.json --detectOpenHandles --coverage && cat ./coverage/lcov.info | coveralls",
    "version": "echo $npm_package_version",
    "changelog": "node changelog.js"
  },
  "keywords": [
    "Blog",
    "STS",
    "Research",
    "Stock markets",
    "Investment",
    "Investment strategies",
    "Economics"
  ],
  "author": "Alexander Padberg <undefinedhuman>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.10",
    "npm": "^6.14.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.12",
    "nodemon": "^2.0.3",
    "commitizen": "^4.0.4",
    "coveralls": "^3.0.11",
    "cz-conventional-changelog": "^3.1.0",
    "jest": "^25.3.0",
    "@types/jest": "^25.2.1",
    "supertest": "^4.0.2",
    "@types/supertest": "^2.0.9",
    "node-sass": "^4.14.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
