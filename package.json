{
  "name": "sts-blog",
  "version": "0.1.12",
  "description": "A private blog in which I document the results of my research on stock markets and various investment strategies.",
  "main": "src/server/index.js",
  "scripts": {
    "commit": "git-cz",
    "start": "node src/server/index.js",
    "dev": "nodemon -q src/server/index.js",
    "test": "jest --config=jest.json --coverage",
    "test-coverage": "jest --config=jest.json --coverage && cat ./coverage/lcov.info | coveralls",

    "version": "echo $npm_package_version",
    "changelog": "node changelog.js"
  },
  "keywords": [
    "Blog",
    "STS",
    "Research",
    "Stock markets",
    "Investment",
    "Investment strategies",
    "Economics"
  ],
  "author": "Alexander Padberg <undefinedhuman>",
  "license": "ISC",
  "devDependencies": {
    "commitizen": "^4.0.4",
    "coveralls": "^3.0.11",
    "cz-conventional-changelog": "^3.1.0",
    "jest": "^25.3.0",

    "@types/jest": "^25.2.1"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "npm": "^6.14.4",

    "@types/express": "^4.17.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
